{
  "name": "astrolicious",
  "compatibility_date": "2025-12-17",
  "compatibility_flags": ["global_fetch_strictly_public"],
  "main": "@astrojs/cloudflare/entrypoints/server",
  "assets": {
    "binding": "ASSETS"
  },
  "images": {
    "binding": "IMAGES"
  },
  "kv_namespaces": [
    {
      "binding": "SESSION"
    }
  ],
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  },
  "routes": [
    {
      "pattern": "astrolicious.dev",
      "custom_domain": true
    },
    {
      "pattern": "www.astrolicious.dev",
      "custom_domain": true
    }
  ]
}
